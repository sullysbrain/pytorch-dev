{
  "name": "Jupyter + Polars (uv)",
  "dockerComposeFile": "../docker-compose.yml",
  "service": "app",
  "workspaceFolder": "/workspace",

  "overrideCommand": false,

  "forwardPorts": [8888],
  "portsAttributes": {
    "8888": { "label": "Jupyter", "requireLocalPort": true, "onAutoForward": "openBrowser" }
  },
  
  "customizations": {
    "vscode": { "extensions": ["ms-python.python", "ms-toolsai.jupyter"] }
  }
}
