{
  "name": "pytorch-dev (local test)",
  "dockerComposeFile": ["../docker-compose.yml"],
  "service": "app",
  "workspaceFolder": "/workspace",
  "shutdownAction": "stopCompose",
  "runServices": ["app"],
  "remoteUser": "comfy",
  "customizations": {
    "vscode": {
      "settings": {
        "terminal.integrated.shell.linux": "/bin/bash"
      }
    }
  },
  "containerEnv": {
    "PYTHONUNBUFFERED": "1"
  },
  "overrideCommand": true
}
